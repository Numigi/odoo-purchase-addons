<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="purchase_order_form_with_product_eta" model="ir.ui.view">
        <field name="name">Purchase Order: add product ETA</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']/tree/field[@name='date_planned']" position="after">
                <field name="product_eta"/>
            </xpath>
        </field>
    </record>

</odoo>
